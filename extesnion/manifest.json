{
  "browser_action": {
    "browser_style": true,
    "default_title": "WCD prober",
    "default_popup":"WCD.html",
    "default_icon": {
      "19": "icons/default19.png",
      "38": "icons/default38.png"
    }
  },
  "description": "WCD prober extension",
  "icons": {
    "48": "icons/cookie.png",
    "96": "icons/cookie@2x.png"
  },
"options_ui": {
    "page": "WCD.html",
    "open_in_tab": true
  },
  "manifest_version": 2,
  "name": "WCD prober",
  "version": "1.0",
  "permissions": ["activeTab",
    "contextualIdentities",
    "cookies",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"]
}
